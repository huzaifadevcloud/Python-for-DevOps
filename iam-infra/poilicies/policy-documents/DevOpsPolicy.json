{
  "Version": "2012-10-17",
  "Id": "DevOpsPolicy",
  "Statement": [
    {
      "Sid": "AllowDevOpsActions",
      "Effect": "Allow",
      "Action": [
       "s3:*"
      ],
      "Resource": "arn:aws:s3:::*/*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "ec2:*"
      ],
      "Resource": "*"
    }
  ]
}
